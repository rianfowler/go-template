{
  "name": "Go Dev Container",
  "build": {
    "dockerfile": "Dockerfile",
    "context": "."
  },
  "customizations": {
    "vscode": {
      "settings": {
        "terminal.integrated.shell.linux": "/bin/zsh"
      },
      "extensions": [
        "golang.Go"
      ]
    }
  },
  "postCreateCommand": "go mod tidy"
}